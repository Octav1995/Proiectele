# 1
print("\t\tProgram de sortare")
a = input("Introduceti nr de elemente ce se vor adauga in lista\n")
lista = []
b = 0
if a.isdigit():
    while b < int(a):
        lista = lista+[input("Introdu elementul\n")]
        b = b+1
else:
    print ("Nu ai introdus un nr")
print ("\t\tLista este afisata\n",lista)
