# 4
sir = input("Introduceti un nr de telefon\n")
if sir.isdigit() and len(sir)==10 and sir.startswith("07"):
    print("Felicitari! Nr. este acceptat.")
elif sir.isdigit():
    print ("Nu ai introdus un nr. valid.")
else:
    print("Caracterele introduse nu sunt cifre")
